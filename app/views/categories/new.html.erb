<h1>New Category</h1>
<%= form_with model: @category do |form| %>
  <div>
    <%= form.label :category_name %><br>
    <%= form.text_field :name %><br>
    <% @category.errors.full_messages_for(:name).each do |message| %>
        <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>